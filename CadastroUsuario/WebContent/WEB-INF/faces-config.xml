<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
	
	<navigation-rule>
		<from-view-id>/cadastroDadosPessoais.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>dadosCormeciais</from-outcome>
			<to-view-id>/cadastroDadosComerciais.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/cadastroDadosComerciais.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>dadosPessoais</from-outcome>
			<to-view-id>/cadastroDadosPessoais.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>dadosAreasInteresse</from-outcome>
			<to-view-id>/cadastroAreasInteresse.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
